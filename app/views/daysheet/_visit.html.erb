<% pat = Patient.find(visit.patient_id) %>

<tr>
	<td><%= link_to visit.entry_ts.strftime("%H:%M"), patient_visit_path(pat,visit) %> </td>
	<td><%= visit.doctor.lname %></td>
	<td><%= link_to pat.full_name, pat %></td>
	<td><%= pat.ohip_num %></td>
	<td><%= pat.ohip_ver %></td>
	<td><%= visit.vis_type %></td>
	<td><%= visit.diag_code %></td>
	<td><%= visit.proc_codes %></td>
	<td><%= visit.units %></td>
	<td><%= BILLING_TYPES.invert[visit.bil_type] %></td>
	<td><%= VISIT_STATUSES.invert[visit.status] rescue '' %></td>
	
	<% if current_user.admin?  %>
		<td> <%= link_to "delete", patient_visit_path(pat,visit), method: :delete, data: { confirm: "Are you sure you want to delete #{visit.entry_ts.strftime('%H:%M')}?" } %> </td>
	<% end %>

</tr>
