<table style="width:1000px; padding: 10px;" >
  <thead>
      <tr>
          <th>Pat Id</th>
 	  <th>Doc Id</th>
          <th>Date</th>
          <th>Notes</th>
          <th>Diag Code</th>
          <th>Proc Code</th>
      </tr>
  </thead>
  <tbody>

  <% @daysheet.each do |v| %>
     <%= render 'visit', visit: v %>
  <% end %>
  
  </tbody>
</table>
</ul>
<p>
<%= will_paginate(@daysheet, :renderer =>  WillPaginate::ActionView::LinkRenderer, class: 'pagination-sm') %>
