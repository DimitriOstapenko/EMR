<h1>Reports</h1>
<% provide(:title, 'All Reports') %>

<table class="tbl table-hover">  <!--table table-hover table-condensed" >  -->
  <thead>
      <tr>
          <th>Name</th>
          <th>Doc_id</th>
          <th>Type</th>
          <th>Sdate</th>
          <th>Edate</th>
          <th></th>
      </tr>
  </thead>
  <tbody>

  <%= render @reports %>

  </tbody>
</table>

<%= will_paginate(@reports) %>

<div class="bottom_buttons">
<div class="col-md-2 col-md-offset-0">
<%= button_to "Create New Report", new_report_path, class: "btn btn-primary", method: 'get'  %>
</div>
</div>

