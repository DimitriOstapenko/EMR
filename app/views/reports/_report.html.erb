    <tr>
	<% if report.filespec.present? && File.exists?(report.filespec) && device_is_desktop? %>
	  <td><%= link_to report.id, report_path(report), remote: true, 'data-toggle':  "modal", 'data-target': '#modal-window' %></td>
	<% else %>
	  <td><%= report.id %></td>
	<% end %>  
	<td><%= report.doctor.lname rescue 'All' %> </td> 
	<td><%= report.name %> </td>
	<% if report.filespec.present? && File.exists?(report.filespec) %>
	  <td><%= link_to '<i class="glyphicon glyphicon-download"></i>'.html_safe, download_report_path(report)  %></td>
	  <td><%= report.filesize %> </td>
        <% else %> <!-- regenerate report -->
	  <td><%= link_to '<i class="glyphicon glyphicon-question-sign"></i>'.html_safe, download_report_path(report)  %></td>
	  <td></td>
  	<% end %>

	<td><%= report.type_str %></td>
	<td><%= report.timeframe_str %></td> 
	
	<td>
	<% if report.timeframe == DAILY_REPORT %>
		<%= "#{report.sdate}" %>
	<% else %>
	   <%= "#{report.sdate} - #{report.edate}" %>
	<% end %>
	</td>

	<td><%= report.created_at %></td>

  	<% if current_user.admin?  %>
	   <td> <%= link_to '<i class="glyphicon glyphicon-trash"></i>'.html_safe, report, method: :delete, data: { confirm: "Are you sure you want to delete #{report.name}?" } %> </td>
	<% end %>

     </tr>
