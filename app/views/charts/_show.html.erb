<%
   provide(:title, "Chart")
%>
 
<div class="modal-header">
	<button type="button" class="close" data-dismiss="modal">&times;</button>
	<b class="modal-title" id="myModalLabel"><%= @chart.patient.full_name %> - Patient Chart : <%= @chart.pages %> pages</b>
</div>
<div class="modal-body">
	<iframe id="myModal" src="/charts/<%= @chart.id %>" frameborder="0" width="100%" height="600px"></iframe>
</div>
<div class="modal-footer">
</div>

