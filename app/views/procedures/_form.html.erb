<%= form_for(@procedure) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <%= f.label :'Code' %>
  <%= f.text_field :code, class: 'form-control' %>

  <%= f.label :'Type' %>
  <%= f.text_field :ptype, class: 'form-control' %>

  <%= f.label :'Description' %>
  <%= f.text_field :descr, class: 'form-control' %>

  <%= f.label :'Cost' %>
  <%= f.text_field :cost, class: 'form-control' %>

  <%= f.label :'Unit' %>
  <%= f.text_field :unit, class: 'form-control' %>

  <%= f.label :fac_req %>
  <%= f.text_field :fac_req, class: 'form-control' %>

  <%= f.label :adm_req %>
  <%= f.text_field :adm_req, class: 'form-control' %>

  <%= f.label :diag_req %>
  <%= f.text_field :diag_req, class: 'form-control' %>

  <%= f.label :ref_req %>
  <%= f.text_field :ref_req, class: 'form-control' %>

  <%= f.label :percent %>
  <%= f.text_field :percent, class: 'form-control' %>

  <%= f.label :eff_date %>
  <%= f.text_field :eff_date, class: 'form-control' %>

  <%= f.label :term_date %>
  <%= f.text_field :term_date, class: 'form-control' %>

  <%= f.submit yield(:button_text), class: "btn btn-primary" %>
<% end %>


