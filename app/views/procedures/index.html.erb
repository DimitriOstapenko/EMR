<% provide(:title, 'All procedures') %>

<h1> List of Procedures </h1>

<ul class="users">
<table style="width:1100px; padding: 10px;" >
  <thead>
      <tr>
          <th>Code</th>
          <th>Type</th>
          <th>Description </th>
	  <th>Cost</th>
	  <th>Unit</th>
	  <th>diag req?</th>
	  <th>Eff Date</th>
	  <th>Active</th>
      </tr>
  </thead>
  <tbody>

  <%= render @procedures %>

  </tbody>


</table>
</ul>
<p>
<%= will_paginate(@procedures) %>

<% if  current_user.admin?  %>
    <section class="new_diag_code">
      <aside class="col-md-2">
            <%= button_to "Add Procedure", new_procedure_path, class: "btn btn-primary", :method => "get" %>
      </aside>
    </section>
<% end %>

