<tr>
  <td><%= link_to visit.date, patient_visit_path(@patient,visit) %></td>
  <td><%= visit.pat_type %></td>
  <td>
  <% if visit.editable? && current_doctor.id == visit.doctor.id %>
    <%= link_to '<i class="glyphicon glyphicon-edit"></i>'.html_safe, edit_patient_visit_path(@patient,visit), remote: true, 'data-toggle':  "modal", 'data-target': '#modal-window', onclick: "set_lg_modal()"  %>
  <% end %>
  </td>
  <td><%= visit.doctor.lname %></td>
  <td><%= visit.diag_scode %></td>
  <td><%= visit.proc_codes %></td>
  <td><%= visit.status_str %></td>
</tr>

