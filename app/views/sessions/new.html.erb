<% provide(:title, "Log in") %>

<div class="col-sm-4 col-sm-offset-4">
<h3 class='display-1'>Sign In</h3>
 <div class="well">

    <%= simple_form_for :session, url: login_path do |f| %>
      <%= f.input :email %>
      <%= f.input :password %>
      <%= f.button :submit, "Log in", class: "btn btn-primary" %>
    <% end %>

 </div>
</div>


