<% provide(:title, @patient.full_name) %>

<div>
<h1> Patient profile </h1>

<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
	   <pre>
	      Name: <%= @patient.lname %>,  <%= @patient.fname %>
	      DOB: <%= @patient.dob %> : <%= @patient.age %> y/o  <%= @patient.sex_str %>
	      Health Card: <%= @patient.ohip_num %> 
	      Phone: <%= @patient.phone %>
           </pre>
      </h1>
    </section>
  </aside>
</div>

</div>
