    <tr>
	<td><%= link_to "#{patient.full_name}", patient %> </td>
	<td><%= patient.sex %></td> 
	<td><%= patient.age_str %></td>
	<td><%= patient.pat_type %> </td>
	<td><%= patient.last_visit_date %> </td> 
	<td><%= patient.visits.count rescue 0 %> </td>
	<td><%= patient.ohip_or_ifh_num_full %> </td>
	<td><%= patient.hin_expiry_short %> </td>
	<td><%= patient.dob %> </td>
	<td><%= num_to_phone(patient.phone) %> </td> 
	<td>
	<% if patient.chart_file.present? %>
	  <%= link_to 'Chart', chart_patient_path(patient), target: 'Label' %>
	<% end %>
	</td>
	
<% remote = device_type == 'desktop' %>
  <td>
  <%= link_to "Label", addrlabel_patient_path(patient), remote: remote, 'data-toggle':  "modal", 'data-target': '#modal-window',  method: 'get'  %>
  <div id="modal-window" class="modal fade  printable autoprint" role="document" aria-labelledby="ModalLabel" aria-hidden="true">
    <div class="modal-dialog " role="dialog">
      <div class="modal-content"></div>
    </div>
  </div>
  </td>

  	<% if current_user.admin?  %>
		<td>  <%= link_to "delete", patient, method: :delete, data: { confirm: "Are you sure you want to delete #{patient.lname}?" } %>  </td> 
	<% end %>

     </tr>

