<% provide(:title, "List of Prescriptions") 
%>
 
<div class="modal-header">
	<button type="button" class="close" data-dismiss="modal">&times;</button>
	<b class="modal-title" id="myModalLabel"><%= @patient.full_name %>: Prescription <%= @prescription.id %> issued  <%= @prescription.date %> </b>
</div>
<div class="modal-body">
	<% if @prescription.pdf_present? %>
	    <object type="application/pdf" data="/prescriptions/<%= @prescription.id %>" width="100%" height="700">No Support</object>
	<% else %>
           <%= render 'body' %>
   	<% end %>
</div>
<div class="modal-footer">
</div>


